<main>

  
  {{#with navigation}}

  <div class="nav">
    {{#each links}}
    <div class="nav-tabs">
    {{#if href}}
  <a class="nav-link" href="{{href}}">{{text}}</a>
  {{else}}
  <a class="nav-link disabled me-5 mb-5" href="#">{{text}}</a>
  {{/if}}
  </div>  
  {{/each}}
  </div>
  {{/with}}


  <div class="text-center text- display-5 mb-5">
    {{header}}
  </div>
  {{#with contacts_data}}
  <div class="display-6 mb-4"> 
    {{title}}
  </div>
  {{#each buttons}}
  {{#if isPrimary}}
  <button class="btn btn-warning ">{{text}}</button>
  {{else}}
  <button class="btn btn-outline-warning">{{text}}</button>
  {{/if}}
  {{/each}}
  {{#each form}}
  {{#with surname}}
 <div class="row g-4 my-1">
  <div class="col-5">
    <label class="form-label">{{label}}</label>
    <input type="text" class="form-control" text-secondary placeholder="{{placeholder}}">
    <div class="form-text text-primary">{{caption}}</div>
  </div>
  {{/with}}
   {{#with name}}
   <div class="col-5">
    <label class="form-label">{{label}}</label>
    <input type="text" class="form-control" text-secondary placeholder="{{placeholder}}">
    <div class="form-text text-primary">{{caption}}</div>
  </div>
    {{/with}}
   {{#with phone}}
   <div class="col-5">
    <label class="form-label">{{label}}</label>
    <input type="text" class="form-control" text-secondary placeholder="{{placeholder}}">
    <div class="form-text text-primary">{{caption}}</div>
  </div>
  {{/with}}
   {{#with email}}
   <div class="col-5">
    <label class="form-label">{{label}}</label>
    <input type="text" class="form-control" text-secondary placeholder="{{placeholder}}">
    <div class="form-text text-primary">{{caption}}</div>
  </div>
  </div>
    
   {{/with}}
    {{/each}}
      

<div>
  <hr class="mt-5">
</div>
{{#each switch}}
{{text}}
{{/each}}
<div>
{{/with}}
</div>
</main>